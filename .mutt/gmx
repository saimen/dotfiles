set mbox_type	=	 Maildir
set folder	=	 "~/mail/gmx"
set spoolfile	=	 "+Inbox"
set record	=	 "+Gesendet"
set postponed	=	 "+drafts"

set from	=	"`echo "" | base64 -d | gpg --armor -r AC18DB3F -d - 2>/dev/null`"

set sendmail 	= 	"/usr/bin/msmtp -a gmx"


my_hdr Reply-To: "`echo "" | base64 -d | gpg --armor -r AC18DB3F -d - 2>/dev/null`"

